<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:descendantFocusability="blocksDescendants"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/SWIS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="362289"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/PRINT_KEY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="0dp"
        android:text="234.00-2-9.00"
        app:layout_constraintLeft_toRightOf="@+id/SWIS"
        app:layout_constraintTop_toTopOf="@+id/SWIS"/>

    <TextView
        android:id="@+id/LOC_ST_NBR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:text="12"
        app:layout_constraintTop_toTopOf="@+id/PRINT_KEY"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"/>

    <TextView
        android:id="@+id/LOC_ST_NAME"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="0dp"
        android:text="Sherrill"
        app:layout_constraintLeft_toRightOf="@+id/LOC_ST_NBR"
        app:layout_constraintTop_toTopOf="@+id/LOC_ST_NBR"/>

    <TextView
        android:id="@+id/LOC_MAIL_ST_SUFF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="0dp"
        android:text="Ln"
        app:layout_constraintLeft_toRightOf="@+id/LOC_ST_NAME"
        app:layout_constraintTop_toTopOf="@+id/LOC_ST_NAME"/>

    <TextView
        android:id="@+id/LOC_MUNI_NAME"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:text="New Hartford"
        app:layout_constraintTop_toTopOf="@+id/LOC_MAIL_ST_SUFF"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.34"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline3"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.58"/>

    <TextView
        android:id="@+id/PARCEL_ID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:text="13"
        app:layout_constraintTop_toTopOf="@+id/PRINT_KEY"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline6"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline6"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.23"/>
</android.support.constraint.ConstraintLayout>